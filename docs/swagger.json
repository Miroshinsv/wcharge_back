{
    "swagger": "2.0",
    "info": {
        "contact": {},
        "version": "2.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/stations": {
            "get": {
                "summary": "Get info about all stations",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/entity.Station"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {}
                    }
                }
            },
            "put": {
                "summary": "Create station",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Station ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Address ID",
                        "name": "Address",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "description": "Free capacity on station",
                        "name": "FreeCapacity",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/entity.Station"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {}
                    }
                }
            },
            "post": {
                "summary": "Create station",
                "parameters": [
                    {
                        "description": "Serial number of station",
                        "name": "SerialNumber",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "Address ID",
                        "name": "Address",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "description": "Full capacity on station",
                        "name": "Capacity",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "description": "Free capacity on station",
                        "name": "FreeCapacity",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/entity.Station"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {}
                    }
                }
            }
        },
        "/stations/{id}": {
            "get": {
                "summary": "Get info about station",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Station ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/entity.Station"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {}
                    }
                }
            }
        }
    },
    "definitions": {
        "entity.Address": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "lat": {
                    "type": "number"
                },
                "lng": {
                    "type": "number"
                }
            }
        },
        "entity.Powerbank": {
            "type": "object",
            "properties": {
                "capacity": {
                    "description": "объем заряда",
                    "type": "number"
                },
                "create_at": {
                    "type": "string"
                },
                "delete_at": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "position": {
                    "type": "integer"
                },
                "removed": {
                    "type": "boolean"
                },
                "serial_number": {
                    "type": "string"
                },
                "update_at": {
                    "type": "string"
                },
                "used": {
                    "description": "сколько уже использована банка в часах // up юзается или нет",
                    "type": "boolean"
                }
            }
        },
        "entity.Station": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "integer"
                },
                "address_full": {
                    "$ref": "#/definitions/entity.Address"
                },
                "capacity": {
                    "type": "number"
                },
                "create_at": {
                    "type": "string"
                },
                "delete_at": {
                    "type": "string"
                },
                "free_capacity": {
                    "type": "number"
                },
                "id": {
                    "type": "integer"
                },
                "powerbanks": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/entity.Powerbank"
                    }
                },
                "removed": {
                    "type": "boolean"
                },
                "serial_number": {
                    "type": "string"
                },
                "update_at": {
                    "type": "string"
                }
            }
        }
    }
}